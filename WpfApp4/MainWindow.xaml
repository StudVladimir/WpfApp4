<Window x:Class="WpfApp4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp4"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="70"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Background="Lime"></Grid>
        <Grid Grid.Row="2" Background="Green"></Grid>
        <Frame Grid.Row="1" NavigationUIVisibility="Hidden" Name="MAinFrame" ContentRendered="MainFrameContentRendered"></Frame>
        <Button Content="Назад" Name="BtnBack" Click="BtnBackClicked" HorizontalAlignment="Right"></Button>
        <TextBlock Text="Автцентр" FontSize="30" VerticalAlignment="Center" HorizontalAlignment="Center"></TextBlock>
        
    </Grid>
</Window>
